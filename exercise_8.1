import numpy as np

def game_core_v3(number: int = 1) -> int:
    """
    Args:
        number (int, optional): Загаданное число. Defaults to 1.

    Returns:
        int: Число попыток
    """
    count = 0

    while True:
        count += 1
        predict_number = np.random.randint(1, 101) # предполагаемое число
        if number > predict_number:
            number -= 10
        if number < predict_number:
            number += 5
        if number > predict_number:
            number -= 1
        if number == predict_number:
            break # выход из цикла, если угадали

    return count

